{
  "name": "xgc/carbon-bundle",
  "license": "MIT",
  "description": "A symfony adaptation of <a href='https://github.com/briannesbitt/Carbon' target='_blank'>nesbot/carbon</a>",
  "keywords": [
    "carbon",
    "symfony"
  ],
  "authors": [
    {
      "email": "xgc1986@gmail.com",
      "name": "Javier GonzÃ¡lez Cuadrado"
    }
  ],
  "type": "library",
  "minimum-stability": "stable",
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Xgc\\CarbonBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
    ]
  },
  "require": {
    "php": ">=7.1",
    "fzaninotto/faker": "^1.7",
    "nesbot/carbon": "^1.22",
    "symfony/yaml": "^3.3",
    "symfony/dependency-injection": "^3.3",
    "doctrine/doctrine-bundle": "^1.7"
  },
  "require-dev": {
    "bruli/php-git-hooks": "^5.1",
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "^0.9.9",
    "matthiasnoback/symfony-dependency-injection-test": "^2.1"
  },
  "scripts": {
    "post-install-cmd": [
      "PhpGitHooks\\Infrastructure\\Composer\\ConfiguratorScript::buildConfig"
    ],
    "post-update-cmd": [
      "PhpGitHooks\\Infrastructure\\Composer\\ConfiguratorScript::buildConfig"
    ]
  },
  "archive": {
    "exclude": [
      "!vendor",
      "tests",
      "*phpunit.xml",
      ".travis.yml",
      "php-git-hooks.yml"
    ]
  }
}
